function twoSum(nums, target) {
  if(!nums || nums.length <= 1 ) return  null;

  let map = new Map();
  let result;


  for(let i=0; i < nums.length; i++){
      let targetedVal = target - nums[i];
      if(map.has(targetedVal)){
          result = [i , map.get(targetedVal)];
        return result
      } else {
        map.set(nums[i], i)
      }
  }
  return result;
}

let nums = [3, 4, 5, 6],
  target = 7;

console.log(twoSum(nums, target));
